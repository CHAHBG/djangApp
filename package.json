{
  "name": "infoapp",
  "version": "1.0.0",
  "description": "Application d'apprentissage de l'informatique pour dÃ©butants",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run react-dev\" \"npm run electron-dev\"",
    "react-dev": "react-scripts start",
    "electron-dev": "wait-on http://localhost:3000 && electron .",
    "build": "react-scripts build",
    "package-win": "electron-builder --win --x64",
    "eject": "react-scripts eject",
    "rebuild": "electron-rebuild"
  },
  "build": {
    "appId": "com.infoapp.app",
    "productName": "InfoApp",
    "files": [
      "build/**/*",
      "src/main.js",
      "src/preload.js",
      "src/enhanced_scraper.py",
      "src/topic_config.py",
      "database/**/*",
      "assets/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.png"
    }
  },
  "dependencies": {
    "antd": "^5.20.6",
    "better-sqlite3": "^11.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "electron": "^31.6.0",
    "electron-builder": "^25.0.5",
    "electron-rebuild": "^3.2.9",
    "wait-on": "^8.0.1",
    "@babel/plugin-transform-private-methods": "^7.25.7",
    "@babel/plugin-transform-numeric-separator": "^7.25.7",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.25.7",
    "@babel/plugin-transform-class-properties": "^7.25.7",
    "@babel/plugin-transform-private-property-in-object": "^7.25.7",
    "@babel/plugin-transform-optional-chaining": "^7.25.7",
    "@eslint/config-array": "^0.18.0",
    "@jridgewell/sourcemap-codec": "^1.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}